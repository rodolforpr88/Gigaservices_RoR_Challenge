<%= form_with(model: user) do |form| %>
  <%= render "shared/error_messages", resource: user %>

  <div>
    <%= form.label :gender, style: "display: block" %>
    <%= form.text_field :gender %>
  </div>

  <div>
    <%= form.label :title, style: "display: block" %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :first, style: "display: block" %>
    <%= form.text_field :first %>
  </div>

  <div>
    <%= form.label :last, style: "display: block" %>
    <%= form.text_field :last %>
  </div>

  <div>
    <%= form.label :email, style: "display: block" %>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
